#!yaml
# Versal instance kinds mapped to a cloud provider's

instance_kinds:
  small:
    # m1.small
    aws: Small Instance

  medium:
    # m1.medium
    aws: Medium Instance

  medium_cpu:
    # c1.medium
    aws: High-CPU Medium Instance

  large:
    # m1.xlarge
    aws: Large Instance

  xlarge:
    # m1.xlarge
    aws: Extra Large Instance

  xlarge_cpu:
    # c1.xlarge
    aws: High-CPU Extra Large Instance
